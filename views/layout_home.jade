  body.mdc-bg-green
    .navbar
      .navbar-inner
        .container-fluid
          a.btn.btn-navbar(data-toggle='collapse', data-target='.top-nav.nav-collapse,.sidebar-nav.nav-collapse')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.brand(href='/index')
            span BLE TASKER
          .nav-no-collapse.header-nav
            ul.nav.pull-right
              li.dropdown
                a.btn.dropdown-toggle(data-toggle='dropdown', href='#')
                  i.halflings-icon.white.user
                  |  Ble Username
                  span.caret
                ul.dropdown-menu
                  li.dropdown-menu-title
                    span Opciones
                  li
                    a(href='/')
                      i.halflings-icon.off
                      |  Salir
    .container-fluid-full
      .row-fluid
        #sidebar-left.span2
          .nav-collapse.sidebar-nav
            ul.nav.nav-tabs.nav-stacked.main-menu
              block menu_options
              li
                a(href='/index')
                  i.fa.fa-home
                  span.hidden-tablet  Inicio
              li
                a(href='/help')
                  i.fa.fa-question
                  span.hidden-tablet  Ayuda
        #content.span10
          ul.breadcrumb
            block breadcrumb
          .mdc-bg-green
            block content

